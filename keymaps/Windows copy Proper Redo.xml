<keymap version="1" name="Windows copy Proper Redo" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt f12" />
    <keyboard-shortcut first-keystroke="ctrl #10000f2" />
  </action>
  <action id="Console.Jdbc.Terminate" />
  <action id="DatabaseView.CloseConnectionAction" />
  <action id="EditorDeleteLine">
    <keyboard-shortcut first-keystroke="ctrl alt x" />
  </action>
  <action id="EditorToggleCase">
    <keyboard-shortcut first-keystroke="ctrl u" />
  </action>
  <action id="GotoSuperMethod" />
  <action id="Images.Editor.ToggleGrid" />
  <action id="IntroduceField" />
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="ctrl alt l" />
    <keyboard-shortcut first-keystroke="ctrl alt f" />
  </action>
  <action id="RenameElement">
    <keyboard-shortcut first-keystroke="shift f6" />
    <keyboard-shortcut first-keystroke="ctrl f2" />
  </action>
  <action id="RunDashboard.Stop" />
  <action id="ShowFilterPopup" />
  <action id="Stop" />
  <action id="XPathView.Actions.Evaluate" />
  <action id="XPathView.Actions.FindByExpression" />
  <action id="XPathView.Actions.ShowPath" />
  <action id="copilot.applyInlays">
    <keyboard-shortcut first-keystroke="tab" second-keystroke="enter" />
  </action>
  <action id="copilot.cycleNextInlays">
    <keyboard-shortcut first-keystroke="alt #10000ec" />
    <keyboard-shortcut first-keystroke="ctrl #10000ec" />
  </action>
  <action id="copilot.cyclePrevInlays">
    <keyboard-shortcut first-keystroke="alt quote" />
    <keyboard-shortcut first-keystroke="ctrl quote" />
  </action>
  <action id="org.intellij.plugins.markdown.ui.actions.styling.InsertImageAction" />
</keymap>